<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pillForZhihu.webApp.dao">
    <class name="User_edge" table="user_edge">
        <id name="entityId" column="user_edge_id" type="java.lang.Long">
            <generator class="identity"/>
        </id>
        <property name="from_user_id" column="from_user_id"/>
        <property name="to_user_id" column="user_pwd"/>
        <property name="user_edge" column="user_edge"/>
    </class>
</hibernate-mapping>