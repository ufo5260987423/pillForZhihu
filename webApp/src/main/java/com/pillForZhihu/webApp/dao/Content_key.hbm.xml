<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pillForZhihu.webApp.dao.utils">
    <class name="KeyBase" table="content_key">
        <id name="keyId" column="user_key_id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
        <property name="key" column="content_key"/>
    </class>
</hibernate-mapping>